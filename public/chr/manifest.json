{
  "name": "AphxTrader",
  "version": "1.0",
  "manifest_version": 2,
  "description": "AphxTrader",
  "homepage_url": "http://www.etrender.se",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "https://www.bitfinex.com/*"
      ],
      "js": [
        "src/inject/inject.js",
        "js/jquery/jquery.min.js"
      ]
    }
  ]
}